<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.pan.szu.pojo">
    <class name="Customer" table="t_customer">
        <id name="id" type="java.lang.Long">
            <generator class="native" />
        </id>
        <property name="name" length="30"></property>
        <property name="sex"></property>
        <property name="birth" type="date"></property>
        <property name="tel" length="15"></property>
        <property name="email" length="50"></property>
        <property name="address" length="100"></property>
        <property name="status"></property>
        <property name="tracecount"></property>
    </class>
</hibernate-mapping>
